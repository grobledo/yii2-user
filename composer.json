{
    "name": "grobledo/yii2-user",
    "description": "Flexible user registration and authentication module for Yii2",
    "keywords": ["yii2", "yii2-user", "user management"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/grobledo/yii2-user/issues?state=open",
        "forum": "https://gitter.im/grobledo/yii2-user",
        "source": "https://github.com/grobledo/yii2-user"
    },
    "authors": [
        {
            "name": "Dmitry Erofeev",
            "email": "dmeroff@gmail.com",
            "homepage": "http://dmeroff.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.15",
        "yiisoft/yii2-swiftmailer": "^2.0.0",
        "yiisoft/yii2-authclient": "^2.1.0",
        "yiisoft/yii2-bootstrap": "^2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "^2.0.0",
        "codeception/specify": "^0.4.3",
        "codeception/verify": "^0.3.1"
    },
    "autoload": {
        "psr-4": {
            "grobledo\\user\\": ""
        }
    },
    "extra": {
        "bootstrap": "grobledo\\user\\Bootstrap"
    }
}
